<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="evanightly.hokimart.EmployeeController">
    <children>
        <TableView fx:id="employeeTable" >
            <columns>
                <TableColumn text="Name" prefWidth="100"  />
                <TableColumn text="Age" prefWidth="100" />
                <TableColumn text="Salary" prefWidth="100" />
                <TableColumn text="Years Experienced" prefWidth="200" />                
                <TableColumn text="Options" prefWidth="100" />
            </columns>
        </TableView>
        <Button fx:id="secondaryButton" text="Logout" onAction="#logout" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
